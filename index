<script>
function myFunction() {
  var x = document.getElementById("myInput").value;
  if (x == "nhanguyendethuonglam") {
    document.getElementById("demo").innerHTML = "Mừng Tuyến về nha. Tuyến cũng cute lắmmm";
  } else {
    document.getElementById("demo").innerHTML = "Meow meow nhập sai ùi kìa :<";
  }
}
